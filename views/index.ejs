
<%- include('header.html') -%>

<!-- MAIN CONTENT -->
<div id="content">

	<div id="carousel" class="carousel slide">
		<ol class="carousel-indicators">
			<li data-target="#carousel" data-slide-to="0" class="active"></li>
			<li data-target="#carousel" data-slide-to="1" class=""></li>
			<li data-target="#carousel" data-slide-to="2" class=""></li>
			<li data-target="#carousel" data-slide-to="3" class=""></li>
		</ol>
		<div class="carousel-inner">
			<div class="item active">
				<img src="/stylesheets/img/index/index_slide1.jpeg" alt="">
			</div>
			<div class="item">
				<img src="/stylesheets/img/index/index_slide2.jpeg" alt="">
			</div>
			<div class="item">
				<img src="/stylesheets/img/index/index_slide3.jpeg" alt="">
			</div>
			<div class="item">
				<img src="/stylesheets/img/index/index_slide4.jpeg" alt="">
			</div>
		</div>
		<a class="left carousel-control" href="#carousel" data-slide="prev"> <span class="glyphicon glyphicon-chevron-left"></span> </a>
		<a class="right carousel-control" href="#carousel" data-slide="next"> <span class="glyphicon glyphicon-chevron-right"></span> </a>
	</div>


</div>
<!-- END MAIN CONTENT -->


<%- include('footer.html') -%>

<style>
	.item img{margin: 0 auto;}
</style>

<!-- PAGE RELATED PLUGIN(S) -->

<script>
    $(document).ready(function() {
        // DO NOT REMOVE : GLOBAL FUNCTIONS!
        pageSetUp();

        $('.carousel.slide').carousel({
            interval : 5000,
            cycle : true
        });

    });

</script>

